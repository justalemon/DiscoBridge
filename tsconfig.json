{
    "compilerOptions": {
        "module": "Node16",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "dist",
        "moduleResolution": "Node16",
        "types": [
            "@citizenfx/client",
            "@citizenfx/server",
            "@types/node",
            "@types/ws"
        ],
        "skipLibCheck": true,
        "strictNullChecks": true
    },
    "include": [
        "*.ts"
    ]
}
